package com.demo.config;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class Data {

	String name="Duga";
	String contact="8791424214";
	
	public Map<String, String> getData(Roles roles){
		Map<String, String> map = new HashMap<>();
		Permission permission = new Permission();
		System.out.println(permission.getPermission());
		Map<String, List<Roles>> m = permission.getPermission();
		for(String str : m.keySet()){
			if(m.get(str).contains(roles))
				//System.out.println(m.get(str));
				map.put(str,  str);
		}
		/*if(roles.equals(Roles.USER)){
		map.put("Name", "Durga");
		map.put("Contact", "8791424214");
		}else{
			map.put("Name", "Durga");
		}*/
		return map;
	}
}

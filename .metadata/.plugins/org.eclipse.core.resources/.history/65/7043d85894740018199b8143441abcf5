
public class SortUsingSingleLoop {

	public static void main(String[] args) {
		int arr[] = {2,6,0,10,2,7,3,2};
		
		int i=0;
		while(i<arr.length-1){
			if(arr[i]>arr[i+1]){
				int temp = arr[i];
				arr[i] = arr[i+1];
				arr[i+1] = temp;
			}
		}
		
		for(Integr i:arr)
			System.out.println(arr[i]);
	}
}
